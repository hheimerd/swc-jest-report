{
    "watchman": false,
    "passWithNoTests": true,
    "roots": [
        "<rootDir>/src"
    ],
    "cacheDirectory": "<rootDir>/node_modules/.cache/jest",
    "collectCoverageFrom": [
        "src/App.tsx"
    ],
    "coverageDirectory": "<rootDir>/__reports/babel",
    "transformIgnorePatterns": [
        "/node_modules/"
    ],
    "moduleDirectories": [
        "node_modules",
        "src"
    ],
    "coverageReporters": [
        "lcov"
    ]
}
